@model WebAtividadeEntrevista.Models.ClienteModel

@{
    ViewBag.Title = "Alterar Cliente";
}
@section scripts{
    <script>
        var urlPost = '@Url.Action("Alterar", "Cliente", new { area = "" })';
        var obj = @Html.Raw(Json.Encode(Model));
        var urlRetorno = '@Url.Action("Index", "Cliente", new { area = "" })';
        var modelBeneficiarios = @Html.Raw(Json.Encode(Model.Beneficiarios));
        var tipoOperacao = "alterar";
    </script>

    <script src="~/Scripts/Beneficiario/beneficiario.js"></script>
    <script src="~/Scripts/Util/mask.js"></script>
    @Scripts.Render("~/bundles/clientes")


